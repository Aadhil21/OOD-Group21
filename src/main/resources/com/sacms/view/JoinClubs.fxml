<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sacms.controllers.JoinClubsController">
   <children>
      <Label text="Join a Club">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label text="Select a club to join" />
      <ListView fx:id="lst_clubs" prefHeight="250.0" prefWidth="300.0" />
      <Button fx:id="btn_joinClub" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onJoinClub" text="Join" />
      <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
